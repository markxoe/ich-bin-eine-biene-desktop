{
    "name": "ichbineinebiene-desktop",
    "description": "Eine App die dir sagt, dass sie eine Biene ist",
    "author": "Mark Oude Elberink",
    "version": "1.1.0",
    "main": "main.js",
    "devDependencies": {
        "electron": "10.1.2",
        "electron-builder": "^22.9.1"
    },
    "scripts": {
        "start": "electron .",
        "build-win": "electron-builder --win --publish never"
    },
    "build": {
        "productName": "Ich bin eine Biene",
        "copyright": "Copyright Â© 2020 Mark Oude Elberink",
        "nsis": {
            "oneClick": "false",
            "installerLanguages": [
                "de_DE",
                "en_US"
            ]
        },
        "win": {
            "target": [{
                "target": "nsis",
                "arch": [
                    "x64",
                    "ia32"
                ]
            }]
        }
    },
    "dependencies": {
        "animejs": "^3.2.1"
    }
}